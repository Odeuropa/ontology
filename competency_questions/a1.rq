PREFIX od: <http://data.odeuropa.org/ontology/>
PREFIX crm: <http://erlangen-crm.org/current/>

# What smell sources have the highest number of documentation in the past?

SELECT ?source COUNT(DISTINCT ?experience)
WHERE {
  ?emission od:F4_had_source / crm:P137_exemplifies ?source;
            od:P1_generated ?smell .

  ?experience od:F2_perceived ?smell.
}
GROUP BY ?source
ORDER BY DESC(COUNT(DISTINCT ?experience))
